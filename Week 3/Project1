   "source": [
    "overall[\"DelayRate\"].plot(kind=\"bar\", rot=0)\n",
    "plt.ylabel(\"Delay Rate\")\n",
    "plt.title(\"Overall Delay Rate by Airline\")\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
