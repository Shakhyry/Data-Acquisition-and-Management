   "source": [
    "pivot[\"Total\"] = pivot[\"Delayed\"] + pivot[\"On Time\"]\n",
    "pivot[\"DelayRate\"] = pivot[\"Delayed\"] / pivot[\"Total\"]\n",
    "pivot\n"
   ]
  },
